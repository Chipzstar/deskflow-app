{
  "name": "deskflow",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "doppler-start": "doppler run -c dev -p deskflow-app -- nx serve",
    "build": "nx build",
    "lint": "nx lint",
    "test": "nx test",
    "e2e": "nx e2e dashboard-e2e",
    "e2e:watch": "nx e2e dashboard-e2e --configuration=watch",
    "prisma:sync": "npx prismerge -i prismerge.json && npx prisma generate && npx prisma db push",
    "prisma:generate": "npx prismerge -i prismerge.json && npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "prisma:push": "npx prisma db push",
    "prisma:pull": "npx prisma db pull",
    "create-env-file:dashboard": "doppler secrets download --no-file --format env > ./apps/dashboard/.env -p deskflow-app -c dev",
    "create-env-file:dashboard:ngrok": "doppler secrets download --no-file --format env > ./apps/dashboard/.env -p deskflow-app -c dev_ngrok",
    "webhookthing": "npx webhookthing@latest",
    "stripe-local-webhook": "stripe listen --forward-to localhost:4200/api/stripe/webhook",
    "ngrok": "ngrok http --domain=deskflow.ngrok.app 4200 --region=eu"
  },
  "private": true,
